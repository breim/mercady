<%= render 'navbars/navbar_institutional' %>
<div class="container">
	<% if notice.present? %>
	<div class="alert alert-info alert-dismissible" role="alert">
		<button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
		<%= notice %>
	</div>
	<% end %>
	<div class="row">
		<%= render 'menus/lateral_users' %>
		<div class="col-lg-9 col-md-9 col-sm-9 col-xs-8">
			<div class="user-container">
				<div class="pull-right">
					<%= link_to "<i class='fa fa-plus-square'></i> Novo Mercado".html_safe, new_market_path, :class => "btn btn-success",data: {no_turbolink: true} %>
				</div>
				<h4>Meus supermercados</h4>
				<hr>
				<%= render 'markets/markets' %>
			</div>
		</div>
	</div>
</div>
